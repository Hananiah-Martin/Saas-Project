<% content_for :title, "Editing artifact" %>

<h1>Editing artifact</h1>

<%= render "form", artifact: @artifact %>

<br>

<div class="mt-4 pt-3 border-top">
  <%= link_to "Show this artifact", artifact_path(@artifact), class: "btn btn-outline-primary" %> |
  
  <%# Change artifacts_path to project_path(@artifact.project) %>
